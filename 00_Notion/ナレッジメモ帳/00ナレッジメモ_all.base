views:
  - type: table
    name: Table
    filters:
      and:
        - file.inFolder("00_Notion/ナレッジメモ帳/memo")
    order:
      - file.name
      - file.basename
      - サブタイトル
      - タグ
      - 作成日
    sort:
      - property: file.name
        direction: ASC
      - property: 作成日
        direction: DESC
      - property: タグ
        direction: ASC
